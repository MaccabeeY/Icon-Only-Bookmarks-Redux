{
   "background": {
      "service_worker": "background.js"
  },
   "action": {
      "default_icon": {
         "128": "images/icon128off.png",
         "16": "images/icon16off.png",
         "32": "images/icon32off.png",
         "48": "images/icon48off.png"
      }
   },
   "description": "A Redux of \"Icon Only Bookmarks\" that hides bookmark name text for a cleaner looking Bookmarks Bar.",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png"
   },
   "manifest_version": 3,
   "name": "Icon Only Bookmarks Redux",
   "permissions": [ "bookmarks", "storage" ],
   "version": "2.0.2"
}
